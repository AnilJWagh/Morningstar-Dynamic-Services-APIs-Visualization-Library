// Import
import Vue from 'vue';
import Vuetify from 'vuetify';

// Utilities
import { shallowMount } from '@vue/test-utils';

// Component
import {{name}} from '@/components/{{dashCase name}}/{{name}}.vue';

Vue.use(Vuetify);

describe('{{name}}.vue', () => {
    it('should render {{titleCase name}}', () => {
        setTimeout(() => {
            const wrapper = shallowMount({{name}});
            expect(wrapper.find('.title h2').text()).toBe('{{titleCase name}}');
        })
    });
});
